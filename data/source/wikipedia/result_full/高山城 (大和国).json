u826f\u770c\u751f\u99d2\u5e02\u306e\u5317\u90e8\u3001[[\u9ed2\u6dfb\u6c60]]\u306e\u5357\u306b\u4f4d\u7f6e\u3059\u308b\u6a19\u9ad8217\u30e1\u30fc\u30c8\u30eb\u3001\u6bd4\u9ad840\u30e1\u30fc\u30c8\u30eb\u306e[[\u5c71\u57ce]]\u3067\u3042\u308b{{sfn|\u5343\u7530|2006|p=56}}\u3002\n\n[[\u6226\u56fd\u6642\u4ee3_(\u65e5\u672c)|\u6226\u56fd\u6642\u4ee3]]\u306e[[\u5927\u548c\u56fd|\u5927\u548c]]\u306e[[\u56fd\u4eba]]\u30fb[[\u9df9\u5c71\u6c0f]]\u306e\u5c45\u57ce\u3067\u3042\u308a\u3001[[\u6cb3\u5185\u56fd]]\u3084[[\u5c71\u57ce\u56fd]]\u3068\u306e\u56fd\u5883\u306b\u4f4d\u7f6e\u3057\u3066\u3044\u305f{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|pp=1, 169}}\u3002\n\n\u57ce\u306f3\u3064\u306e[[\u66f2\u8f2a]]\u304b\u3089\u69cb\u6210\u3055\u308c\u3066\u304a\u308a\u3001[[2006\u5e74]]\u3001\u6587\u5316\u8ca1\u4fdd\u8b77\u306e\u89b3\u70b9\u304b\u3089[[\u907a\u69cb]]\u3092\u907f\u3051\u308b\u3088\u3046\u306a\u5f62\u3067\u30cf\u30a4\u30ad\u30f3\u30b0\u9053\u304c\u8a2d\u7f6e\u3055\u308c\u305f{{sfn|\u5343\u7530|2006|p=56}}\u3002\n<!--==\u57ce\u306e\u7bc4\u56f2==-->\n\n== \u6cbf\u9769 ==\n\u9df9\u5c71\u57ce\uff08\u9ad8\u5c71\u57ce\uff09\u306e\u3042\u308b\u5927\u548c\u56fd\u6dfb\u4e0b\u90e1\u9df9\u5c71\u8358{{efn|[[\u5ef6\u4e45]]2\u5e74\uff08[[1070\u5e74]]\uff09\u306e\u300c\u8208\u798f\u5bfa\u96d1\u5f79\u514d\u5e33\u300d\u306b\u300c\u9df9\u5c71\u5e84\u300d\u306e\u540d\u304c\u898b\u3089\u308c\u308b{{sfn|\u751f\u99d2\u5e02\u8a8c\u7de8\u7e82\u59d4\u54e1\u4f1a|1985|p=291}}\u3002\u307e\u305f\u9ad8\u5c71\u60e3\u3001\u9ad8\u5c71\u5e84\u3001\u9ad8\u5c71\u90f7\u3001\u9ad8\u5c71\u6751\u306a\u3069\u3001\u53e4\u304f\u304b\u3089\u9df9\u5c71\u6751\u3068\u3068\u3082\u306b\u4f75\u79f0\u3055\u308c\u3066\u304d\u305f{{sfn|\u751f\u99d2\u5e02\u8a8c\u7de8\u7e82\u59d4\u54e1\u4f1a|1985|p=291}}\u3002}}\u306f\u3001\u5927\u548c\u306e\u8ecd\u52e2\u304c\u5317\u6cb3\u5185\u306b\u5c55\u958b\u3059\u308b\u969b\u306b\u901a\u904e\u3057\u305f\u308a\u3001[[\u5948\u826f]]\u304b\u3089[[\u4eac\u90fd]]\u3078\u5411\u304b\u3046\u50e7\u304c\u5c71\u57ce\u56fd[[\u76f8\u697d\u90e1]]\u306b\u629c\u3051\u308b\u305f\u3081\u306b\u5ea6\u3005\u901a\u308b\u306a\u3069\u3059\u308b\u3001\u4ea4\u901a\u306e\u8981\u885d\u3060\u3063\u305f{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=170}}\u3002\n\n\u3053\u306e\u5730\u3092\u62bc\u3055\u3048\u308b\u56fd\u4eba\u30fb\u9df9\u5c71\u6c0f\u306f\u3001[[\u5fdc\u6c38]]27\u5e74\uff08[[1420\u5e74]]\uff09\u306b\u9df9\u5c71\u8358\u306e[[\u4e0b\u53f8|\u4e0b\u53f8\u8077]]\u3092\u5f97\u3066\u3044\u308b\u3053\u3068\u304c\u78ba\u8a8d\u3067\u304d\u308b<ref>{{Citation|\u548c\u66f8|editor=\u300c\u89d2\u5ddd\u65e5\u672c\u5730\u540d\u5927\u8f9e\u5178\u300d\u7de8\u7e82\u59d4\u54e1\u4f1a|year=1990|title=[[\u89d2\u5ddd\u65e5\u672c\u5730\u540d\u5927\u8f9e\u5178]] 29 \u5948\u826f\u770c|publisher=[[\u89d2\u5ddd\u66f8\u5e97]]|isbn=4-04-001290-9|page=662}}</ref>\u3002[[\u5609\u5409]]4\u5e74\uff08[[1444\u5e74]]\uff09\u306b\u306f[[\u9df9\u5c71\u5965\u983c\u5f18]]\u304c[[\u7560\u5c71\u6301\u56fd]]\u3092\u6483\u9000\u3057\u3066\u304a\u308a{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=170}}\u3001[[\u5eb7\u6b63]]\u5143\u5e74\uff08[[1455\u5e74]]\uff09\u3001\u983c\u5f18\u306f[[\u8208\u798f\u5bfa]][[\u4e00\u4e57\u9662]]\u65b9\u306e[[\u8846\u5f92|\u5b98\u7b26\u8846\u5f92]]\u3068\u306a\u3063\u3066\u3044\u308b{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|pp=137, 170}}\u3002[[\u5fdc\u4ec1\u306e\u4e71]]\uff08[[1467\u5e74|1467]] - [[1477\u5e74]]\uff09\u3067\u897f\u8ecd\u306b\u4ed8\u3044\u305f\u9df9\u5c71\u6c0f\u306f\u3001[[\u6587\u660e (\u65e5\u672c)|\u6587\u660e]]9\u5e74\uff081477\u5e74\uff09\u306b\u306f[[\u8d8a\u667a\u6c0f (\u5927\u548c\u56fd)|\u8d8a\u667a\u6c0f]]\u30fb[[\u53e4\u5e02\u6c0f]]\u306b\u6b21\u3050\u897f\u8ecd\u306e\u4e3b\u529b\u52e2\u529b\u3068\u898b\u306a\u3055\u308c\u3066\u3044\u305f{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|pp=170\u2013171}}\u3002\u305d\u306e\u305f\u3081\u304b\u3001\u540c\u5e7411\u6708\u3001\u897f\u8ecd\u306e\u7dcf\u5927\u5c06\u3060\u3063\u305f[[\u8db3\u5229\u7fa9\u8996]]\u306f[[\u7f8e\u6fc3\u56fd|\u7f8e\u6fc3]]\u3078\u306e\u4e0b\u56fd\u524d\u3001\u4e00\u6642\u9df9\u5c71\u3092\u5fa1\u5ea7\u6240\u3068\u3057\u305f<ref>\u300c[[\u5c0b\u5c0a]]\u5927\u50e7\u6b63\u8a18\u300d\u6587\u660e9\u5e74\uff081477\u5e74\uff0911\u670814\u65e5\u6761\uff08\u300e\u5927\u4e57\u9662\u5bfa\u793e\u96d1\u4e8b\u8a18\u300f\uff09\u3002</ref>{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=171}}\u3002\n\n[[\u660e\u5fdc]]7\u5e74\uff08[[1498\u5e74]]\uff09\u3001\u9df9\u5c71\u6c0f\u3068\u59fb\u621a\u95a2\u4fc2\u306b\u3042\u308b{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=174}}[[\u53e4\u5e02\u6f84\u80e4]]\u304c\u9df9\u5c71\u57ce\u306b\u62e0\u3063\u3066\u6226\u3044\u3001\u8fd1\u90ca\u306e[[\u7af9\u6797\u5bfa (\u751f\u99d2\u5e02)|\u7af9\u6797\u5bfa]]\u304c\u713c\u304b\u308c\u3066\u3044\u308b{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=172}}\u3002\u9df9\u5c71\u57ce\u306e\u7bc9\u57ce\u6642\u671f\u306f\u4e0d\u660e\u3060\u304c\u3001\u3053\u306e\u6642\u306e\u300e[[\u5927\u4e57\u9662\u5bfa\u793e\u96d1\u4e8b\u8a18]]\u300f\u306e\u8a18\u4e8b\u306b\u300c\u9ad8\u5c71\u57ce\u300d\u306e\u540d\u524d\u304c\u73fe\u308c\u308b{{efn|\u300c\u5c0b\u5c0a\u5927\u50e7\u6b63\u8a18\u300d\u660e\u5fdc7\u5e74\uff081498\u5e74\uff098\u67086\u65e5\u6761\uff08\u300e\u5927\u4e57\u9662\u5bfa\u793e\u96d1\u4e8b\u8a18\u300f\uff09\u306b\u300c\u53e4\u5e02\u30cf\u9ad8\u5c71\u57ce\u30cb\u5728\u4e4b\u3001\u5408\u6226\u3001\u79cb\u7be0\u30fb\u5b9d\u6765\u8846\u30c8\u4e5f\u3001\u6240\u3005\u713c\u4e4b\u300d\u3068\u3042\u308b{{sfn|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=155|loc=\u95a2\u9023\u8cc7\u6599117}}<ref>{{Citation|\u548c\u66f8|year=1981|chapter=|title=[[\u65e5\u672c\u6b74\u53f2\u5730\u540d\u5927\u7cfb]]30 \u5948\u826f\u770c\u306e\u5730\u540d|publisher=[[\u5e73\u51e1\u793e]]}}</ref>\u3002}}\u3002\n\n[[16\u4e16\u7d00]]\u4e2d\u9803\u306e\u9df9\u5c71\u6c0f\u5f53\u4e3b\u30fb[[\u9df9\u5c71\u5f18\u983c]]\u306f\u6cb3\u5185\u56fd[[\u5b88\u8b77]]\u306e[[\u7560\u5c71\u6c0f#\u7560\u5c71\u5c3e\u5dde\u5bb6|\u7560\u5c71\u6c0f]]\u306b\u5c5e\u3057\u3001\u5317\u6cb3\u5185\u3067\u306e\u8ecd\u4e8b\u52d5\u54e1\u3092\u671f\u5f85\u3055\u308c\u3001\u307e\u305f[[\u5b89\u898b\u5b97\u623f]]\u3068\u3068\u3082\u306b\u5357\u5c71\u57ce\u306e[[\u5b88\u8b77\u4ee3]]\u306b\u4efb\u3058\u3089\u308c\u308b\u306a\u3069\u3057\u305f{{sfnm|\u5c0f\u8c37|2015|1p=320|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|2p=175}}\u3002\u3057\u304b\u3057[[\u5929\u6587 (\u5143\u53f7)|\u5929\u6587]]22\u5e74\uff08[[1553\u5e74]]\uff09\u3001\u7560\u5c71\u6c0f\u306b\u304a\u3051\u308b\u6a29\u529b\u4e89\u3044\u306e\u4e2d\u3001\u5f18\u983c\u306f\u5b89\u898b\u5b97\u623f\u3068\u5bfe\u7acb\u3057\u3066\u81ea\u5203{{sfnm|\u5c0f\u8c37|2015|1p=321|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|2pp=175, 189\u2013190}}\u3002\u5f18\u983c\u306e\u5b50\u30fb[[\u9df9\u5c71\u85e4\u653f|\u85e4\u653f]]\u306f\u7560\u5c71\u6c0f\u304b\u3089\u96e2\u308c\u3001\u9df9\u5c71\u306f\u5b97\u623f\u306e\u6240\u9818\u306b\u306a\u3063\u305f\u3082\u306e\u3068\u307f\u3089\u308c\u308b{{sfn|\u5c0f\u8c37|2015|p=322}}\u3002[[\u6c38\u7984]]2\u5e74\uff08[[1559\u5e74]]\uff09\u3001\u5b97\u623f\u3068\u6575\u5bfe\u3059\u308b[[\u4e09\u597d\u9577\u6176]]\u306e\u5bb6\u81e3\u30fb[[\u677e\u6c38\u4e45\u79c0]]\u304c\u5927\u548c\u306b\u4fb5\u653b\u3057\u3001\u9df9\u5c71\u306f\u713c\u304d\u8a0e\u3061\u306b\u3042\u3063\u305f<ref>\u300c\u4eab\u7984\u5929\u6587\u4e4b\u8a18\u300d\u6c38\u79842\u5e7410\u670812\u65e5\u6761\u306b\u300c\u5f3e\u6b63\u6bbf\u9df9\u5c71\u3078\u52e2\u9063\u713c\u6255\u300d\u3068\u3042\u308b\uff08{{harvnb|\u5c0f\u8c37|2015|loc=\u53f2\u659959}}\uff09\u3002</ref>{{sfn|\u5c0f\u8c37|2015|p=322}}\u3002\u3053\u306e\u6642\u306b\u713c\u3051\u843d\u3061\u305f\u3082\u306e\u304b{{efn|\u6587\u660e6\u5e74\uff08[[1474\u5e74]]\uff09\u306e\u6226\u706b\u3067\u713c\u5931\u3057\u305f\u3068\u3082\u3055\u308c\u308b{{sfnm|\u91ce\u53e3|1974|1pp=101, 105|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|2p=2}}\u3002}}\u3001[[\u5143\u4e80]]3\u5e74\uff08[[1572\u5e74]]\uff09\u3001[[\u9df9\u5c71\u85e4\u9038]]\u306b\u3088\u308a[[\u9ad8\u5c71\u516b\u5e61\u5bae]]\u304c\u518d\u5efa\u3055\u308c\u305f{{sfnm|\u5c0f\u8c37|2015|1p=322|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|2pp=2, 175}}\u3002\n\n\u3053\u306e\u5f8c\u3001\u9df9\u5c71\u6c0f\u306f[[\u7b52\u4e95\u9806\u6176]]\u30fb[[\u7b52\u4e95\u5b9a\u6b21|\u5b9a\u6b21]]\u306b\u4ed5\u3048\u3001[[\u5929\u6b63]]13\u5e74\uff08[[1585\u5e74]]\uff09\u306e\u5b9a\u6b21\u306e[[\u4f0a\u8cc0\u56fd|\u4f0a\u8cc0]]\u8ee2\u5c01\u306b\u3068\u3082\u306a\u3044\u9df9\u5c71\u3092\u53bb\u308b\u3053\u3068\u3068\u306a\u308b{{sfnm|\u751f\u99d2\u5e02\u8a8c\u7de8\u7e82\u59d4\u54e1\u4f1a|1985|1p=90|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|2p=1}}\u3002\u9df9\u5c71\u57ce\u306e\u6b63\u78ba\u306a\u5ec3\u57ce\u6642\u671f\u306f\u5206\u304b\u3089\u306a\u3044\u304c\u3001\u9df9\u5c71\u6c0f\u5f53\u4e3b\u306f\u3053\u306e\u6642\u307e\u3067\u9df9\u5c71\u57ce\u306b\u5728\u57ce\u3057\u3066\u3044\u305f\u3068\u3055\u308c\u308b<ref>\u300c\u9df9\u5c71\u6c0f\u7cfb\u56f3\u5199\u300d\uff08{{harvnb|\u751f\u99d2\u5e02\u6559\u80b2\u59d4\u54e1\u4f1a|2020|p=76}}\uff09\u3002</ref>\u3002\n\n== \u69cb\u9020 ==\n\u9ad8\u5c71\u57ce\u306f\u6a19\u9ad8217\u30e1\u30fc\u30c8\u30eb\u3001\u6bd4\u9ad840\u30e1\u30fc\u30c8\u30eb\u306e\u4e18\u9675\u4e0a\u306b\u3042\u308b\u5c71\u57ce\u3067\u3001\u5357\u5317\u306b\u5ef6\u3073\u308b\u5c3e\u6839\u306b\u6cbf\u3063\u3066\u7d04200\u30e1\u30fc\u30c8\u30eb\u306b\u308f\u305f\u308a\u4e3b\u8981\u306a\u66f2\u8f2a\u304c\u9023\u306a\u3063\u3066\u3044\u308b\u3002\u5927\u304d\u304f\u5206\u3051\u3066\u4e09\u3064\u306e\u90e8\u5206\u304b\u3089\u306a\u3063\u3066\u304a\u308a\u3001\u5317\u304b\u3089\u9806\u306b\u305d\u308c\u305e\u308c\u4e2d\u5fc3\u66f2\u8f2a\u7fa41\u3001\u66f2\u8f2a2\u3001\u66f2\u8f2a3\u3068\u3059\u308b{{sfn|\u5343\u7530|2006|p=56}}\u3002\n\n'''\u4e2d\u5fc3\u66f2\u8f2a\u7fa41'''\n:\u6a19\u9ad8217\u30e1\u30fc\u30c8\u30eb\u306e\u6700\u9ad8\u6240\u306b\u3042\u308b\u66f2\u8f2a\u3068\u305d\u308c\u3092\u4e2d\u5fc3\u3068\u3059\u308b\u66f2\u8f2a\u7fa4\u3002\u9ad8\u5c71\u57ce\u5168\u4f53\u306e\u4e3b\u90ed\u3068\u3044\u3048\u308b\u3002\u5317\u5074\u306b\u4f4e\u3044\u571f\u5841\u304c\u307f\u3089\u308c\u3001\u305d\u306e\u5148\u306e\u978d\u90e8\u306b\u306f\u81ea\u7136\u5730\u5f62\u3068\u5224\u5225\u3057\u3065\u3089\u3044\u7de9\u3044\u5800\u5207\u304c\u3042\u308b{{sfn|\u5343\u7530|2006|pp=56\u201357}}\u3002\n\n'''\u66f2\u8f2a2'''\n:\u4e5d\u982d\u9f8d\u738b\u3092\u7940\u308b\u5341\u4e09\u91cd\u5854\u304c\u8a2d\u7f6e\u3055\u308c\u3066\u304a\u308a\u30012006\u5e74\u306b\u6574\u5099\u3055\u308c\u305f\u30cf\u30a4\u30ad\u30f3\u30b0\u9053\u306e\u7d42\u70b9\u306b\u5f53\u305f\u308b{{sfn|\u5343\u7530|2006|pp=57\u201358}}\u3002\n\n:\u4e2d\u5fc3\u66f2\u8f2a\u7fa4\u306e\u5357\u306b\u96a3\u63a5\u3059\u308b\u3088\u3046\u306b\u4f4d\u7f6e\u3057\u3001\u4e2d\u5fc3\u66f2\u8f2a\u7fa4\u3068\u306e\u9593\u306b\u5800\u5207\u306a\u3069\u306f\u898b\u3048\u305a\u906e\u65ad\u306f\u3055\u308c\u3066\u3044\u306a\u3044\u3002\u305d\u306e\u305f\u3081\u4e2d\u5fc3\u66f2\u8f2a\u7fa4\u306e\u5357\u5074\u306e\u5b88\u308a\u3092\u56fa\u3081\u308b\u5f79\u5272\u3092\u62c5\u3044\u3001\u4e00\u4f53\u3068\u306a\u3063\u3066\u904b\u7528\u3055\u308c\u305f\u3068\u898b\u3089\u308c\u308b\u3002\u307e\u305f\u66f2\u8f2a2\u306e\u5357\u5074\u659c\u9762\u306f\u65ad\u5d16\u72b6\u306e\u6025\u50be\u659c\u3068\u306a\u308a\u3001\u8981\u5bb3\u6027\u304c\u9ad8\u3044{{sfn|\u5343\u7530|2006|pp=57\u201358}}\u3002\n\n'''\u66f2\u8f2a3'''\n:\u66f2\u8f2a2\u304b\u3089\u5357\u306b\u3084\u3084\u96e2\u308c\u305f\u4f4d\u7f6e\u306b\u3042\u308b\u3002\u5317\u5074\u30fb\u6771\u5074\u306b\u306f\u571f\u5841\u304c\u5de1\u308b\u304c\u3001\u5468\u56f2\u306f\u81ea\u7136\u5730\u5f62\u3070\u304b\u308a\u3067\u306f\u3063\u304d\u308a\u3068\u3057\u305f\u5800\u5207\u306e\u8de1\u304c\u898b\u3089\u308c\u306a\u3044\u3001\u57ce\u90ed\u3068\u3057\u3066\u306f\u4e0d\u5b8c\u5168\u306a\u5f62\u614b\u3068\u306a\u3063\u3066\u3044\u308b{{sfn|\u5343\u7530|2006|p=58}}\u3002\n\n\u9ad8\u5c71\u57ce\u306e\u3053\u306e\u69cb\u9020\u306f\u3001\u660e\u5fdc7\u5e74\uff081498\u5e74\uff09\u306e\u53e4\u5e02\u6c0f\u306e\u5165\u57ce\u306e\u969b\u3001\u81e8\u6642\u306e\u99d0\u5c6f\u5730\u3068\u3057\u3066\u5f62\u4f5c\u3089\u308c\u305f\u3082\u306e\u3067\u3042\u308b\u53ef\u80fd\u6027\u304c\u3042\u308b\u3002\u305d\u3046\u3067\u3042\u308c\u3070\u9632\u5fa1\u306e\u305f\u3081\u306e\u53b3\u91cd\u306a\u5800\u306e\u5fc5\u7136\u6027\u3082\u306a\u304f\u3001\u9ad8\u5c71\u57ce\u306b\u660e\u77ad\u306a\u5800\u5207\u304c\u306a\u3044\u3053\u3068\u3068\u3082\u5408\u81f4\u3059\u308b{{sfn|\u5343\u7530|2006|p=58}}\u3002\n\n\u307e\u305f\u3001\u540c\u76df\u7684\u306a\u8ecd\u52e2\u306b\u3088\u308b\u7bc9\u57ce\u306f\u4e3b\u90ed\u4e2d\u5fc3\u306e\u6c42\u5fc3\u6027\u3092\u6c42\u3081\u305f\u69cb\u9020\u3067\u306a\u304f\u3001\u66f2\u8f2a\u304c\u5206\u7acb\u7684\u306b\u96c6\u5408\u3059\u308b\u6a2a\u4e26\u3073\u306e\u69cb\u9020\u3068\u306a\u308b\u3002\u540c\u76df\u7684\u95a2\u4fc2\u306e\u53e4\u5e02\u6c0f\u304c\u9ad8\u5c71\u57ce\u306b\u5165\u57ce\u3057\u3066\u3044\u308b\u3053\u3068\u3084\u3001\u9df9\u5c71\u5f18\u983c\u304c\u7387\u3044\u305f\u6cb3\u5185\u8846\u3082\u540c\u76df\u7684\u95a2\u4fc2\u306b\u3042\u3063\u305f\u3082\u306e\u3068\u8003\u3048\u3089\u308c\u308b\u3053\u3068\u304b\u3089\u3001\u9df9\u5c71\u6c0f\u306e\u6a29\u529b\u69cb\u9020\u306f\u540c\u76df\u7684\u69cb\u9020\u306b\u3088\u308b\u3082\u306e\u3068\u898b\u306a\u305b\u3001\u9ad8\u5c71\u57ce\u306e\u57ce\u90ed\u69cb\u9020\u3082\u305d\u308c\u3092\u53cd\u6620\u3057\u305f\u3082\u306e\u3067\u3042\u308b\u3068\u8003\u3048\u3089\u308c\u308b{{sfn|\u5343\u7530|2006|p=58}}\u3002\n<!--== \u73fe\u5728\u78ba\u8a8d\u53ef\u80fd\u306a\u6b8b\u5b58\u907a\u69cb ==-->\n\n== \u6240\u5728\u5730 ==\n* \u751f\u99d2\u5e02\u9ad8\u5c71\u753a\u5e84\u7530\u5b57\u57ce\u8336\u5c4b{{sfn|\u6751\u7530\u307b\u304b|1980|p=321}}\n